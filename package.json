{
	"name": "@illux/exolix",
	"private": true,
	"version": "0.1.0",
	"description": "A NodeJS framework",
	"main": "",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/IlluxDev/Exolix.git"
	},
	"scripts": {
		"build": "ts-node build.ts",
		"start": "ts-node test.ts",
		"format": "prettier --write .",
		"compile": "yarn format && yarn build",
		"setup:dev": "npm run format && npm run install:rest && npm run install:terminal && npm run install:socket && npm run install:utils",
		"install:rest": "cd rest && yarn && cd ..",
		"install:terminal": "cd terminal && yarn && cd ..",
		"install:socket": "cd socket && yarn && cd ..",
		"install:utils": "cd utils && yarn && cd .."
	},
	"author": "Illux",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/IlluxDev/Exolix/issues"
	},
	"homepage": "https://github.com/IlluxDev/Exolix#readme",
	"devDependencies": {
		"@types/node": "^16.11.7",
		"prettier": "^2.4.1",
		"ts-node": "^10.4.0",
		"typescript": "^4.4.4",
		"yarn": "^1.22.17"
	}
}
